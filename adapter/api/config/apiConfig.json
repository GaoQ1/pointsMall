{
    "/mall" : {
        "template":"index",
        "title":"积分商城",
        "config":[
            {
                "baseurl":"BASE_INTEGRATION",
                "method":"GET",
                "command":"integral/indexcard",
                "data":{},
                "auth":"true",
                "includeAppId":"true"
            }]
    },
    "/mall/commodityList" : {
        "template":"commodityList",
        "title":"商品列表",
        "config":[
            {
                "baseurl":"BASE_INTEGRATION",
                "method":"GET",
                "command":"integral/commonditylist",
                "data":{"categoryId":"query.categoryId"},
                "auth":"true",
                "includeAppId":"true"
            }]
    },
    "/mall/commodityDetail" : {
        "template":"commodityDetail",
        "title":"商品详情",
        "config":[
            {
                "baseurl":"BASE_INTEGRATION",
                "method":"GET",
                "command":"integral/commonditydetail",
                "data":{"productId":"query.productId"},
                "auth":"true",
                "includeAppId":"true"
            }]
    },
    "/mall/changeList" : {
        "template":"changeList",
        "title":"兑换记录",
        "config":[
            {
                "baseurl":"BASE_INTEGRATION",
                "method":"GET",
                "command":"integral/recordlist",
                "data":{"pageNow":"query.pageNow","pageSize":"query.pageSize"},
                "auth":"true",
                "includeAppId":"true"
            }]
    },
    "/mall/getscoreList" : {
        "template":"getscoreList",
        "title":"积分详情",
        "config":[
            {
                "baseurl":"BASE_INTEGRATION",
                "method":"GET",
                "command":"integral/getscorelist",
                "data":{"pageNow":"query.pageNow","pageSize":"query.pageSize"},
                "auth":"true",
                "includeAppId":"true"
            }]
    },
    "/proexchange" : {
        "title":"商品兑换",
        "config":[
            {
                "baseurl":"BASE_INTEGRATION",
                "method":"GET",
                "command":"integral/commondityexchange",
                "data":{"productId": "query.productId", "score": "query.score"},
                "auth":"true",
                "includeAppId":"true"
            }]
    },
    "/bindAccessToken":{
        "title":"绑定接口",
        "config":[
            {
                "baseurl":"BASE_INTEGRATION",
                "method":"POST",
                "command":"integral/bind",
                "data":{"openId": "query.openId"},
                "auth":"true"
            }]
    },
    "/login":{
        "title":"登录",
        "config":[
            {
                "baseurl":"BASE_PASSPORT_URL",
                "method":"POST",
                "command":"login",
                "data":"body",
                "auth":"false",
                "encrypt":"true",
                "includeAppId":"true"
            }]
    },
    "/getsmscode":{
        "title":"获取短信验证码",
        "config":[
            {
                "baseurl":"BASE_PASSPORT_URL",
                "method":"POST",
                "command":"sendSMS",
                "data":"body",
                "auth":"false",
                "encrypt":"true",
                "includeAppId":"true"
            }]
    },
    "/imgcode":{
        "title":"获取图形验证码",
        "config":[
            {
                "baseurl":"API_URL",
                "method":"GET",
                "command":"commonSys/validatecode?verycode={body.imgCode}&phone={body.phone}",
                "auth":"false",
                "encrypt":"false",
                "data":{}
            }]
    },
    "/weiXinLogin" : {
        "title":"微信登录",
        "config":[
            {
                "baseurl":"BASE_PASSPORT_URL",
                "method":"POST",
                "command":"xxxxxx",
                "auth":"false"
            }]
    },
    "/appLogin" : {
        "title":"app登录",
        "config":[
            {
                "baseurl":"BASE_PASSPORT_URL",
                "method":"POST",
                "command":"xxxxxxxxx",
                "auth":"false"
            }]
    }
}